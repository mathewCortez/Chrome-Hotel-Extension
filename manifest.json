{
    "manifest_version": 2,
    "name": "Hotel Accurate",
    "description": "Gives you the real price of the hotels after the taxes and fees",
    "version":"0.1",
    "author":"Mathew Cortez",
    "content_scripts": [
        {
            "matches": [
                "https://www.google.com/travel/hotels/*",  "http://www.google.com/travel/hotels/*"
            ],
          "run_at": "document_end",
            "js": ["jquery-3.4.1.min.js","jquery-ui.min.js","content.js"],
          "css": ["jquery-ui.min.css"]
        }
    ],
    "browser_action": {
        "default_icon": "icon.png"
    },
    "background": {
      "scripts": ["background.js"]
    }
}

